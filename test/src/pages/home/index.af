{
  "type": "module",
  "imports": [
    {
      "src": "./index.module.scss",
      "modules": [
        {
          "name": "default",
          "alias":"styles"
        }
      ]
    }
  ],
  "variables": {
    "theme": {
      "type": "string",
      "reactive": true,
      "value": "dark"
    },
    "num": {
      "type": "number",
      "value": 0,
      "reactive": true
    }
  },
  "view": {
    "name": "div",
    "props": {
      "className": "${styles.home} ${styles[theme]}"
    },
    "children": [
      {
        "name": "InfoBar"
      },
      {
        "name": "HelloWorld",
        "events": {
          "onClickChangeTheme": {
            "type": "setVariable",
            "name": "theme",
            "values": [
              "${theme === 'dark' ? 'light' : 'dark'}"
            ]
          }
        }
      },
      {
        "name": "div",
        "props": {
          "style": {
            "padding": 10,
            "display": "flex",
            "alignItems": "center",
            "gap": 10
          }
        },
        "children": [
          {
            "name": "Switch",
            "props": {
              "text": "-1"
            },
            "events": {
              "onClick": {
                "type": "script",
                "code": "console.log(styles)",
                "values": []
              }
            }
          },
          {
            "name": "a",
            "text": "${(num+'').padStart(10, '0')}"
          },
          {
            "name": "Switch",
            "props": {
              "text": "+1"
            },
            "events": {
              "onClick": {
                "type": "handler",
                "src": "./increase.js",
                "values": []
              }
            }
          }
        ]
      }
    ]
  }
}