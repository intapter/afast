{
  "type": "module",
  "variables": {
    "hello": {
      "type": "string",
      "value": "Hello"
    },
    "name": {
      "type": "string",
      "reactive": true,
      "value": "world"
    }
  },
  "events": {
    "onClickChangeTheme": {}
  },
  "view": {
    "name": "div",
    "props": {
      "style": {
        "padding": "10px"
      }
    },
    "children": [
      {
        "name": "div",
        "text": "${hello} ${name}!"
      },
      {
        "name": "div",
        "props": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "marginTop": 10,
            "gap": 10
          }
        },
        "children": [
          {
            "name": "Switch",
            "props": {
              "text": "Change Word"
            },
            "events": {
              "onClick": {
                "type": "setVariable",
                "name": "name",
                "values": [
                  "${name === 'Afast' ? 'World' : 'Afast'}"
                ]
              }
            }
          },
          {
            "name": "Switch",
            "props": {
              "text": "Change Theme"
            },
            "events": {
              "onClick": {
                "type": "dispatch",
                "name": "onClickChangeTheme"
              }
            }
          },
          {
            "name": "Switch",
            "props": {
              "text": "console.log()"
            },
            "events": {
              "onClick": {
                "type": "script",
                "src": "./demo.js",
                "values":[]
              }
            }
          }
        ]
      }
    ]
  }
}
