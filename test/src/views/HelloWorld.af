{
  "type": "module",
  "fields": {
    "hello": {
      "type": "string",
      "value": "Hello"
    },
    "name": {
      "type": "string",
      "reactive": true,
      "value": "world"
    }
  },
  "events": {
    "onClickChangeTheme": {}
  },
  "view": {
    "name": "div",
    "props": {
      "style": {
        "padding": "10px"
      }
    },
    "children": [
      {
        "name": "TestA"
      },{
        "name": "TestB"
      },
      {
        "name":"Switch",
        "props":{
          "text":"点我"
        },
        "events":{
          "onClick":{
            "type":"nav",
            "value":"/about"
          }}
      },
      {
        "name": "div",
        "text": "${hello} ${name}!"
      },
      {
        "name": "div",
        "props": {
          "style": {
            "display": "flex",
            "alignItems": "center",
            "marginTop": 10,
            "gap": 10
          }
        },
        "children": [
          {
            "name": "Switch",
            "props": {
              "text": "Change Word"
            },
            "events": {
              "onClick": {
                "type": "setField",
                "name": "name",
                "values": [
                  "${name === 'Afast' ? 'World' : 'Afast'}"
                ]
              }
            }
          },
          {
            "name": "Switch",
            "props": {
              "text": "Change Theme"
            },
            "events": {
              "onClick": {
                "type": "dispatch",
                "name": "onClickChangeTheme"
              }
            }
          }
        ]
      }
    ]
  }
}
