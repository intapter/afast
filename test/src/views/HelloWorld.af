{
  "type": "module",
  "variables": {
    "hello": {
      "type": "string",
      "value": "Hello"
    },
    "name": {
      "type": "string",
      "reactive": true,
      "value": "world"
    }
  },
  "view": {
    "name": "div",
    "props": {
      "style": {
        "display": "flex",
        "padding": "10px"
      }
    },
    "children": [
      {
        "name": "h1",
        "text": "${hello} ${name}!"
      }
    ]
  }
}
